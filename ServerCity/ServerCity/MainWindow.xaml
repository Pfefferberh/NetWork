<Window x:Class="ServerCity.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ServerCity"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="250">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="35*"/>
            <RowDefinition Height="38*"/>
            <RowDefinition Height="146*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="tbIndex" />
        <Label Name="lbCity" Grid.Row="1" Margin="2"/>
        <Button x:Name="btnSend" Content="Resive Data" Grid.Row="2" Margin="20" Click="btnSend_Click"/>
    </Grid>
</Window>
